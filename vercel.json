{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "rewrites": [
        {
            "source": "/favicon.ico",
            "destination": "https://juicy.vercel.app/favicon.ico"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ]
}
